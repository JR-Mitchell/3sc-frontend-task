{
  "name": "3sc-frontend-task",
  "version": "1.0.0",
  "description": "Building a simple web UI for PokeAPI for the 3 Sided Cube frontend task",
  "main": "index.js",
  "config": {
    "port": "8080"
  },
  "scripts": {
    "test": "echo \"No test specified.\" && exit 1",
    "build": "webpack --config wp.config.js",
    "dev": "if [ ! -z \"$npm_config_port\" ]; then webpack-dev-server --config wp.config.js --mode development --port $npm_config_port; else npm run dev --port=$npm_package_config_port; fi"
